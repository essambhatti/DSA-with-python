    "def insertion_sort(arr):\n",
    "    for i in range(1, len(arr)):\n",
    "        j = i \n",
    "        while arr[j-1] > arr[j] and j>0:\n",
    "            arr[j], arr[j-1] = arr[j-1], arr[j]\n",
    "            j-=1\n",
    "            print(arr)\n",
    "    return arr\n",
    "    \n",
    "print(insertion_sort([5,4,3,2,1]))"
   ]
  }
